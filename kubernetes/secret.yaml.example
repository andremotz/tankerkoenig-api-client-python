apiVersion: v1
kind: Secret
metadata:
  name: logger-secrets
  namespace: default  # Anpassen falls nötig
type: Opaque
stringData:
  api-key: "YOUR_TANKERKOENIG_API_KEY_HERE"  # Dein Tankerkoenig API-Key
  influxdb-token: "YOUR_INFLUXDB_TOKEN_HERE"  # InfluxDB Token (optional, falls Auth benötigt)

